apiVersion: v1
kind: Config
clusters:
- cluster:
    certificate-authority: {{ pki_dir }}/ca.crt
    server: https://192.168.56.11:6443
  name: kubernetes
contexts:
- context:
    cluster: kubernetes
    user: kubelet
  name: kubelet@kubernetes
current-context: kubelet@kubernetes
users:
- name: kubelet
  user:
    client-certificate: {{ pki_dir }}/kubelet.crt
    client-key: {{ pki_dir }}/kubelet.key